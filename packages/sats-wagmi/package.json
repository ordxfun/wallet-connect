{
  "name": "@ordxfun/wallet-connect",
  "version": "0.3.25",
  "description": "BTC wallet connectors and React hooks for wallet-connect.",
  "homepage": "https://github.com/ordxfun/wallet-connect#readme",
  "license": "MIT",
  "keywords": [
    "wallet-connect",
    "btc wallet connector",
    "btc wallet connector hooks",
    "btc wallet connector config",
    "ordxfun"
