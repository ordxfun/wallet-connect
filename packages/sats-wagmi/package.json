    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/bs58check": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@bitcoin-js/tiny-secp256k1-asmjs": "^2.2.3",
    "@gobob/bob-sdk": "3.1.7-alpha0",
